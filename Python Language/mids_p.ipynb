      "source": [
        "# To take string from user and create dictionary with characters from given string as key and count each characters as its value\n",
        "\n",
        "s1 = input(\"Enter string :\")\n",
        "dic = {}\n",
        "for ele in s1:\n",
        "    dic[ele] = dic.get(ele, 0)+1\n",
        "print(dic)"
      ]
