      "source": [
        "# Q-77 --> Apython program to call super class constructor in the sub class using super ()\n",
        "\n",
        "class Class():\n",
        "  def __init__(self,x):\n",
        "    print(x)\n",
        "class SubClass(Class):\n",
        "  def __init__(self,x):\n",
        "    super().__init__(x)\n",
        "\n",
        "x = [1, 2.9, 3.4, 4, 5]\n",
        "a = SubClass(x)"
      ]
    }
